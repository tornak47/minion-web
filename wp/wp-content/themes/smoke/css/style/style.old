/* HTML Options */
html {}
body {font-family: Arial, Helvetica, sans-serif; font-size:12px; line-height:19px; color:#fefefe; background-color:#15161a;}
a { color:#f9f9f9; text-decoration:none;}
a:hover { color:#bababa; text-decoration:none;}

